			onAction="#nextQuote" prefHeight="31.0" prefWidth="153.0" text="Next Quote"
			GridPane.columnIndex="7" GridPane.columnSpan="2" GridPane.rowIndex="5" />
		<Label fx:id="sceneTitleLabel" alignment="CENTER"
			contentDisplay="CENTER" prefHeight="46.0" prefWidth="599.0"
			text="Quotation in Computer Science" textAlignment="CENTER"
			GridPane.columnSpan="9">
			<font>
				<Font name="Arial" size="32.0" />
