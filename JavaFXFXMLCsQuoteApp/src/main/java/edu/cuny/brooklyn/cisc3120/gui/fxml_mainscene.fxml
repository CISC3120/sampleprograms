			<padding>
				<Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
			</padding>
		</Label>
		<StackPane prefHeight="76.0" prefWidth="124.0"
			GridPane.columnSpan="2" GridPane.rowIndex="1" GridPane.rowSpan="2">
